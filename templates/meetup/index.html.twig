{% extends 'base.html.twig' %}

{% block title %}Hobby!{% endblock %}

{%block stylesheets %}{{ encore_entry_link_tags('globalStyle') }}
{{encore_entry_link_tags('meetupStyle')}}
{%endblock%}

{% block body %}
<div class="container">
{% for m in meetups %}
<div class="meetupcontainer">
<div class="item1">
{% if m.nom =="JUG Montpellier"%}
<img src="https://secure.meetupstatic.com/photos/event/d/3/2/600_481143378.jpeg" alt="jug">
{% elseif m.nom =="Montpellier JS" %}
<img src="data:image/png;base64," alt="js">
{% elseif m.nom =="Sunny Tech" %}
<img src="data:image/jpeg;base64," alt="st">
{% else %}
<img src="data:image/jpeg;base64," alt="mth">
{% endif %}
</div>
<div class="item2">
<h1>{{m.nom}}</h1>
<a href={{m.lien}}>{{m.lien}}</a></br>
</div>
</div>
{% endfor %}
</div>
{% endblock %}
